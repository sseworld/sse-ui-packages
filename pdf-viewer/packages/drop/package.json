{
    "name": "@sse-ui/pdf-drop",
    "version": "1.0.0",
    "description": "A React component to view a PDF document",
    "license": "MIT",
    "author": {
        "name": "SSE World",
        "email": "help@world.sse",
        "url": "https://sseworld.github.io/"
    },
    "keywords": [
        "react",
        "react.js",
        "pdf",
        "pdf.js",
        "pdf viewer"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/cjs/*.*",
        "lib/styles/*.*",
        "lib/index.d.ts",
        "lib/index.js"
    ],
    "dependencies": {
        "@sse-ui/pdf-core": "1.0.0"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "clean": "rm -rf lib && mkdir lib",
        "build": "npm run clean && npm run js && npm run css",
        "js": "cp src/index.d.ts lib && cp dist/index.js lib && rollup -c ../../rollup.config.cjs",
        "j1": "rollup -c ../../rollup.config.cjs",
        "css": "sass --no-source-map --style compressed src/styles/index.scss lib/styles/index.css",
        "release": "npm publish --access public"
    }
}
